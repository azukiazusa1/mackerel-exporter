{
  "name": "mackerel-exporter",
  "version": "0.0.4",
  "description": "Open Telemetry Metrics Exporter for Mackerel",
  "type": "module",
  "main": "build/cjs/src/index.js",
  "types": "build/cjs/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/esm/src/index.js",
      "require": "./build/cjs/src/index.js",
      "types": "./build/cjs/src/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "build:watch": "tsc -w",
    "dev": "ts-node --transpile-only src/index.ts",
    "clean": "rimraf build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "prepare": "npm run clean && npm run build"
  },
  "keywords": [
    "opentelemetry",
    "node.js",
    "metrics",
    "mackerel"
  ],
  "author": "azukiazusa1",
  "license": "MIT",
  "devDependencies": {
    "@swc/core": "^1.3.80",
    "@swc/helpers": "^0.5.1",
    "@tsconfig/node-lts-strictest-esm": "^18.12.1",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "regenerator-runtime": "^0.14.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vitest": "^0.34.3"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.1",
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/core": "^1.15.2",
    "@opentelemetry/exporter-metrics-otlp-grpc": "^0.41.2",
    "@opentelemetry/sdk-metrics": "^1.15.2",
    "@opentelemetry/semantic-conventions": "^1.15.2"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azukiazusa1/mackerel-exporter.git"
  },
  "bugs": {
    "url": "https://github.com/azukiazusa1/mackerel-exporter/issues"
  },
  "homepage": "https://github.com/azukiazusa1/mackerel-exporter#readme",
  "files": [
    "build"
  ]
}
