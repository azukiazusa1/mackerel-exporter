{
  "name": "mackerel-exporter",
  "version": "0.0.5",
  "description": "Open Telemetry Metrics Exporter for Mackerel",
  "type": "module",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup ./src",
    "build:watch": "tsc -w",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "prepare": "npm run clean && npm run build"
  },
  "keywords": [
    "opentelemetry",
    "node.js",
    "metrics",
    "mackerel"
  ],
  "author": "azukiazusa1",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node-lts-strictest-esm": "^18.12.1",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.3"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.1",
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/core": "^1.15.2",
    "@opentelemetry/exporter-metrics-otlp-grpc": "^0.41.2",
    "@opentelemetry/sdk-metrics": "^1.15.2",
    "@opentelemetry/semantic-conventions": "^1.15.2"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azukiazusa1/mackerel-exporter.git"
  },
  "bugs": {
    "url": "https://github.com/azukiazusa1/mackerel-exporter/issues"
  },
  "homepage": "https://github.com/azukiazusa1/mackerel-exporter#readme",
  "files": [
    "build"
  ]
}
